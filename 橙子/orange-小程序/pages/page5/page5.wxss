.logo {
  width: 260rpx;
  height: 260rpx;
  border-radius: 50%;
  margin: 160rpx auto;
  border: 2px solid #FF4500;
  animation: dh 200s linear infinite;
}

@keyframes dh {
  0%{transform: rotate(0deg)}
  100%{transform: rotate(7200deg)}
}

.logo image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.tex {
  width: 360rpx;
  font-size: 40rpx;
  font-weight: 600;
  margin: 20rpx auto;
  color: #FF4500; 
}